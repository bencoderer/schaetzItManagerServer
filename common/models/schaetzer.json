{
  "name": "Schaetzer",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "operatorKey": {
      "type": "string",
      "required": true
    },
    "nameAndAddress": {
      "type": "string",
      "required": true
    },
    "idInOperatorDb": {
      "type": "string",
      "required": true
    },
    "indate": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "schaetzungen": {
      "type": "embedsMany",
      "model": "Schaetzung",
      "property": "schaetzungList",
      "options": {
        "validate": true,
        "forceId": true
      }
    },
    "sync": {
      "type": "hasMany",
      "model": "SchaetzerSyncToOperator",
      "property": "syncToOperatorList",
      "options": {
        "validate": true,
        "forceId": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
